sum of digits 
  #include <stdio.h>

void sumOfDigits(int n) {
    if (n < 10 || n > 99) {
        printf("Invalid Input\n");
    } else {
        int sum = (n / 10) + (n % 10);
        printf("Sum of Digit %d is %03d\n", n, sum);
    }
}

int main() {
    int n;
    scanf("%d", &n);
    sumOfDigits(n);
    return 0;
}
