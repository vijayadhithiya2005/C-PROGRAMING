area and perimeter of triangle
#include<stdio.h>
int main()
{
    int a,b,c,d,e;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    scanf("%d",&d);
    scanf("%d",&e);
    float area=0.5*a*b;
    scanf("%2f\n",&area);
    float perimeter=c+d+e;
    scanf("%2f\n",&perimeter);
    printf("Area of Triangle is %.2f\n",area);
    printf("Perimeter of Triangle is %.2f\n",perimeter);
    return 0;
        
}
