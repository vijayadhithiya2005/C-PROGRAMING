delete the vowels
  #include <stdio.h>
#include <string.h>
#include <ctype.h>

int is_vowel(char c) {
    c = tolower((unsigned char)c);
    return (c=='a' || c=='e' || c=='i' || c=='o' || c=='u');
}

int main() {
    char input[1000];
    char output[1000];
    int i, j = 0;
    if (!fgets(input, sizeof(input), stdin)) return 0;
    size_t len = strlen(input);
    if (len > 0 && input[len - 1] == '\n') input[len - 1] = '\0';
    for (i = 0; input[i] != '\0'; i++) {
        if (!is_vowel(input[i])) {
            output[j++] = input[i];
        }
    }
    output[j] = '\0';

    printf("%s\n", output);
    return 0;
}
