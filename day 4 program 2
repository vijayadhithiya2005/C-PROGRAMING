delete the blank space
  #include <stdio.h>
#include <string.h>

void removeSpaces(char *str) {
    int i = 0, j = 0;
    while (str[i]) {
        if (str[i] != ' ') {
            str[j++] = str[i];
        }
        i++;
    }
    str[j] = '\0';
}

int main() {
    char str[1000];

    if (fgets(str, sizeof str, stdin)) {
        
        size_t len = strlen(str);
        if (len > 0 && str[len-1] == '\n')
            str[len-1] = '\0';

        removeSpaces(str);
        printf("%s", str);
    }
    return 0;
}
