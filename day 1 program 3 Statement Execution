Statement Execution
#include<stdio.h>
int main()
{
    int a;
    float b;
    char c[50];
    char d[50];
    char e[200];
    scanf("%d",&a);
    scanf("%f",&b);
    scanf("%s",c);
    scanf("%s",d);
    scanf("%[^\n]s",e);
    printf("Integer is %d\n",a);
    printf("Float is %.2f\n",b);
    printf("Character is %s\n",c);
    printf("Strings are %s and %s\n",d,e);
    return 0;
}
