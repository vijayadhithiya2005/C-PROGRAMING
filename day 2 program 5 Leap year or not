leap year or not 
#include <stdio.h>

int main() {
    int year;
    scanf("%d", &year);

    if ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0)) {
        printf("The Given Year %d is a Leap Year.", year);
    } else {
        printf("The Given Year %d is Not a Leap Year.", year);
    }

    return 0;
}

  
