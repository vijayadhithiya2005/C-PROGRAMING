series 2
  #include <stdio.h>


int isPrime(int n) {

    if (n <= 1) return 0;

    for (int i = 2; i * i <= n; i++) {

        if (n % i == 0) return 0;

    }

    return 1;

}


void generatePrimeSeries(int n) {

    int count = 0;

    int num = 101;

    while (count < n) {

        if (isPrime(num)) {

            printf("%d ", num);

            count++;

        }

        num++;

    }

}

int main() {

    int n;

    scanf("%d", &n);

    generatePrimeSeries(n);

    return 0;

}
