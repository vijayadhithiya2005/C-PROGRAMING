Reverse of string
  #include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    if (fgets(s, sizeof(s), stdin)) {
        s[strcspn(s, "\n")] = '\0';
        
        int i = 0;
        int j = strlen(s) - 1;
        char temp;
        while (i < j) {
            temp = s[i];
            s[i] = s[j];
            s[j] = temp;
            i++;
            j--;
        }
        printf("%s", s);
    }

    return 0;
}
