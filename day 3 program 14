maximum product of 2 element in an array
  #include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int nums[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    int max1 = 0, max2 = 0;

    for (int i = 0; i < n; i++) {
        if (nums[i] > max1) {
            max2 = max1;
            max1 = nums[i];
        } else if (nums[i] > max2) {
            max2 = nums[i];
        }
    }
    int maxProduct = (max1 - 1) * (max2 - 1);
    printf("Maximum product of two elements in an array is %d\n", maxProduct);

    return 0;
}

