concatenate the string
  #include <stdio.h>
#include <string.h>

int main() {
    char s1[1000], s2[1000];
    fgets(s1, sizeof(s1),stdin);
    fgets(s2, sizeof(s2),stdin);
    s1[strcspn(s1,"\n")] = '\0';
    s2[strcspn(s2,"\n")] = '\0';

    
    strcat(s1,s2);  
    printf("%s",s1);
    return 0;
}
